/// Input  : 5
/// Output : # 1 * # 2 * # 3 * # 4 * # 5 *

#include <stdio.h>
#include <conio.h>

void Pattern( int iNo )
{
            int i = 0;
            
            for( i = 1; i <= iNo; i++ )
            {
                        printf(" # \t %d * \t",i);
            }
            
            return;
}

int main()
{
            int iValue = 0;
  
            printf("\n Enter Number Of Elements => ");
            scanf("%d",&iValue);
            
            Pattern( iValue );
            
            getch();
            return 0;
}
